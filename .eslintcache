path=\"/\">\n          <Home />\n        </Route>\n        <Route exact path=\"/menu\">\n          <Menu menuData={menuData} cartItems={cartItems} addToCart={addToCart} removeFromCart={removeFromCart}/>\n        </Route>\n        <Route exact path=\"/reviews\">\n          <Reviews/>\n        </Route>\n        <Route exact path=\"/shoppingcart\">\n          <ShoppingCart cartItems={cartItems} numberOfItems={numberOfItems} removeFromCart={removeFromCart}/>\n        </Route>\n      </Switch>\n    </Container>\n    </>\n  );\n}\n\nexport default App;\n","/home/tej317/Development/code/phase-2/taste-of-india-menu/src/Navbar.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/Home.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/Menu.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/Reviews.js",["59","60","61","62"],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/ShoppingCart.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/DisplayPage.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/AboutUs.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/Footer.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/Menudata.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/MenuCards.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/FoodItem.js",[],{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":1,"message":"68","line":2,"column":26,"nodeType":"69","messageId":"70","endLine":2,"endColumn":35},{"ruleId":"67","severity":1,"message":"71","line":3,"column":20,"nodeType":"69","messageId":"70","endLine":3,"endColumn":23},{"ruleId":"67","severity":1,"message":"72","line":3,"column":25,"nodeType":"69","messageId":"70","endLine":3,"endColumn":28},{"ruleId":"67","severity":1,"message":"73","line":3,"column":30,"nodeType":"69","messageId":"70","endLine":3,"endColumn