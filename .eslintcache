"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/index.js",[],["56","57"],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/App.js",["58"],"import \"./App.css\";\nimport {React, useState, useEffect} from \"react\";\nimport { Route, Switch } from \"react-router-dom\";\nimport { Container } from 'react-bootstrap';\nimport Navbar from \"./Navbar\";\nimport Home from \"./Home\";\nimport Menu from \"./Menu\";\nimport Reviews from \"./Reviews\";\nimport ShoppingCart from \"./ShoppingCart\";\nimport menuData from \"./Menudata\";\n\nfunction App() {\n\n  const [cartItems, setCartItems] = useState([])\n  const numberOfItems = cartItems.reduce((total, item) => (total + item.quantity),0) \n  \n  function addToCart(id) {\n    const selectedItem = cartItems.find((item) => item.id === id)\n    if (selectedItem) {\n      const newCartItems = cartItems.map((item) => {\n        if (item.id === id) {\n          return {...item, quantity: item.quantity+1}\n        }\n        else {\n          return item\n        }\n      })\n      setCartItems(newCartItems)\n    } \n    else {\n      const newCartItem = menuData.find((item) => item.id === id)\n      newCartItem.quantity = 1\n      const newCartItems = [...cartItems, newCartItem]\n      setCartItems(newCartItems)\n    }\n  }\n\n  function removeFromCart(id) {\n    const selectedItem = cartItems.find((item) => item.id === id)\n\n    if (selectedItem.quantity > 1) {\n      const newCartItems = cartItems.map((item) => {\n        if (item.id === id) {\n          return {...item, quantity: item.quantity-1}\n        }\n        else {\n          return item\n        }\n      })\n      setCartItems(newCartItems)\n    } \n    else {\n      const newCartItems = cartItems.filter((item) => item.id !== id)\n      setCartItems(newCartItems)\n    }\n  }\n\n  return (\n    <>\n    <Navbar numberOfItems={numberOfItems}/>\n    <Container fluid style={{ paddingLeft: 0, paddingRight: 0 }}>\n      <Switch>\n        <Route exact path=\"/\">\n          <Home />\n        </Route>\n        <Route exact path=\"/menu\">\n          <Menu menuData={menuData} cartItems={cartItems} addToCart={addToCart} removeFromCart={removeFromCart}/>\n        </Route>\n        <Route exact path=\"/reviews\">\n          <Reviews/>\n        </Route>\n        <Route exact path=\"/shoppingcart\">\n          <ShoppingCart cartItems={cartItems} numberOfItems={numberOfItems} removeFromCart={removeFromCart}/>\n        </Route>\n      </Switch>\n    </Container>\n    </>\n  );\n}\n\nexport default App;\n","/home/tej317/Development/code/phase-2/taste-of-india-menu/src/Navbar.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/Home.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/Menu.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/Reviews.js",["59","60","61","62","63","64","65","66","67","68","69","70","71"],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/ShoppingCart.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/DisplayPage.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/AboutUs.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/Footer.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/Menudata.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/MenuCards.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/FoodItem.js",[],{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","severity":1,"message":"77","line":2,"column":26,"nodeType":"78","messageId":"79","endLine":2,"endColumn":35},{"ruleId":"76","severity":1,"message":"80","line":1,"column":16,"nodeType":"78","messageId":"79","endLine":1,"endColumn":24},{"ruleId":"76","severity":1,"message":"81","line":3,"column":20,"nodeType":"78","messageId":"79","endLine":3,"endColumn":23},{"ruleId":"76","severity":1,"message":"82","line":3,"column":25,"nodeType"