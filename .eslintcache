iner } from 'react-bootstrap';\nimport Navbar from \"./Navbar\";\nimport Home from \"./Home\";\nimport Menu from \"./Menu\";\nimport Reviews from \"./Reviews\";\nimport ShoppingCart from \"./ShoppingCart\";\nimport menuData from \"./Menudata\";\n\nfunction App() {\n\n  const [cartItems, setCartItems] = useState([])\n  const numberOfItems = cartItems.reduce((total, item) => (total + item.quantity),0) \n  \n  function addToCart(id) {\n    const selectedItem = cartItems.find((item) => item.id === id)\n    if (selectedItem) {\n      const newCartItems = cartItems.map((item) => {\n        if (item.id === id) {\n          return {...item, quantity: item.quantity+1}\n        }\n        else {\n          return item\n        }\n      })\n      setCartItems(newCartItems)\n    } \n    else {\n      const newCartItem = menuData.find((item) => item.id === id)\n      newCartItem.quantity = 1\n      const newCartItems = [...cartItems, newCartItem]\n      setCartItems(newCartItems)\n    }\n  }\n\n  function removeFromCart(id) {\n    const selectedItem = cartItems.find((item) => item.id === id)\n\n    if (selectedItem.quantity > 1) {\n      const newCartItems = cartItems.map((item) => {\n        if (item.id === id) {\n          return {...item, quantity: item.quantity-1}\n        }\n        else {\n          return item\n        }\n      })\n      setCartItems(newCartItems)\n    } \n    else {\n      const newCartItems = cartItems.filter((item) => item.id !== id)\n      setCartItems(newCartItems)\n    }\n  }\n\n  return (\n    <>\n    <Navbar numberOfItems={numberOfItems}/>\n    <Container fluid style={{ paddingLeft: 0, paddingRight: 0 }}>\n      <Switch>\n        <Route exact path=\"/\">\n          <Home />\n        </Route>\n        <Route exact path=\"/menu\">\n          <Menu menuData={menuData} cartItems={cartItems} addToCart={addToCart} removeFromCart={removeFromCart}/>\n        </Route>\n        <Route exact path=\"/reviews\">\n          <Reviews/>\n        </Route>\n        <Route exact path=\"/shoppingcart\">\n          <ShoppingCart cartItems={cartItems} numberOfItems={numberOfItems} removeFromCart={removeFromCart}/>\n        </Route>\n      </Switch>\n    </Container>\n    </>\n  );\n}\n\nexport default App;\n","/home/tej317/Development/code/phase-2/taste-of-india-menu/src/Navbar.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/Home.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/Menu.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/Reviews.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/ShoppingCart.js",["57"],"import React from \"react\";\nimport {Container} from 'react-bootstrap'\nimport Footer from \"./Footer\";\n\nfunction ShoppingCart({cartItems, numberOfItems, removeFromCart}) {\n\n    const shoppingCartComponents = cartItems.map((item) => {\n        return (\n            <div className=\"row border border-3 m-3 p-0 border-warning\" key={item.id}>\n                <div className=\"col-md-3\"><img className=\"rounded m-3\" src={item.url} width=\"100\"/></div>\n                <div className=\"col-md-4 my-auto h4\">Palak Panner</div>\n                    <div className=\"col-md-2 my-auto h5\">Quantity: {item.quantity}</div>\n                    <div className=\"col-md-1 my-auto h5\">${item.price}</div>\n                    <div className=\"col-md-2 my-auto h5\">\n                        <button className=\"btn btn-danger btn-lg py-2\" onClick={() => removeFromCart(item.id)}>Remove</button>\n                    </div>\n            </div>\n        )\n    })\n    \n    let price = parseFloat(cartItems.reduce((total, item) => (total + item.price*item.quantity),0).toFixed(2))\n    let tax = parseFloat((price*0.08).toFixed(2))\n    let total = parseFloat((price+tax).toFixed(2))\n\n    function checkedOut() {\n        alert(\"Your order has been placed. Thank you!\")\n    }\n\n    return (\n        <>\n            <Container className=\"shoppingcartcontainer1 d-flex align-items-center justify-content-center mt-3 \">\n                <div className=\"shoppingcartcontainer2 mt-5 border border-dark border-5\">\n                    <h2 className=\"mb-0 mx-3 my-2\">Shopping Cart</h2>\n                    <div className=\"d-flex justify-content-between\">\n                        <p className=\"mx-3 my-2\">You have {numberOfItems} items in your cart</p>\n                    </div>\n                    {shoppingCartComponents}\n                    <div>\n                        <h5 className=\"prices\">Subtotal: ${price}</h5>\n                        <h5 className=\"prices\">Tax(8%): ${tax}</h5>\n                        <h3 className=\"prices\">Total: ${total}</h3>\n                        <button type=\"button\" className=\"btn btn-success btn-lg checkoutbutton mb-4\" onClick={checkedOut}>Checkout\n                        </button>\n                    </div>\n                </div>\n            </Container>\n            <Footer className=\"shpcartfoot\"></Footer>\n        </>\n    )\n}\n\nexport default ShoppingCart;","/home/tej317/Development/code/phase-2/taste-of-india-menu/src/DisplayPage.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/AboutUs.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/Footer.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/Menudata.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/MenuCards.js",[],"/home/tej317/Development/code/phase-2/taste-of-india-menu/src/FoodItem.js",[],{"ruleId":"58","severity":1,"message":"59","line":2,"column":26,"nodeType":"60","messageId":"61","endLine":2,"endColumn":35},{"ruleId":"62","severity":1,"message":"63","line":10,"column":43,"nodeType":"64","endLine":10,"endColumn":100},"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative 